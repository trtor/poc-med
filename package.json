{
  "name": "poc-med",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/trtor/poc-med.git",
  "author": "trtor",
  "license": "MIT",
  "private": true,
  "scripts": {
    "install:prod": "rm -rf node_modules/ && npm install --only=production",
    "check": "prettier --check \"{src,test}/**/*.{js,jsx,json,md,ts,tsx,html,css}\"",
    "format": "prettier --write \"{src,test}/**/*.{js,jsx,json,md,ts,tsx,html,css}\"",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix",
    "dev": "nodemon",
    "build": "rm -rf dist/ && tsc -p .",
    "copy-csv": "mkdir -p dist/csv && copyfiles --error --up 1 csv/*.* dist/csv",
    "start": "npm run build && node dist/index.js",
    "start:prod": "yarn env-cmd -f .env node dist/src/app/index.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watchAll --passWithNoTests",
    "test:watch:cov": "jest --watchAll --coverage --passWithNoTests"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "ioredis": "^5.2.0",
    "redis": "^4.2.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.1"
}
